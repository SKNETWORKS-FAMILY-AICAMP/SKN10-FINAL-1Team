def calculate_risk_info(df):
    numeric_cols = df.select_dtypes(include=['float64', 'int64']).columns
    categorical_cols = df.select_dtypes(include=['object', 'category']).columns
    
    numeric_info = df[numeric_cols].mean().to_dict()
